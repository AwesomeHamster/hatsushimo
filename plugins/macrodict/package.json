        "puppeteer"
      ],
      "optional": [],
      "implements": []
    },
    "locales": [
      "en",
      "zh",
      "ja"
    ],
    "recommendeds": []
  },
  "devDependencies": {
    "@hamster-bot/constructeur": "*",
    "@koishijs/plugin-database-memory": "^1.3.1",
    "@koishijs/plugin-mock": "^1.0.6",
    "@types/node": "^17.0.5",
    "@types/pug": "^2.0.6",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.7.3",
    "yml-register": "^1.0.0"
  },
  "peerDependencies": {
    "@koishijs/plugin-puppeteer": "^3.0.0-rc.0",
    "koishi": "^4.4.0"
  },
  "dependencies": {
